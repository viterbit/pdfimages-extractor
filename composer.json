{
  "name": "viterbit/pdfimages-extractor",
  "description": "Extract images from pdf",
  "keywords": [
    "viterbit",
    "pdfimages",
    "extractor"
  ],
  "homepage": "https://github.com/viterbit/pdfimages-extractor",
  "license": "MIT",
  "require": {
    "php" : "^7.3|^8.0",
    "symfony/process": "^4.0|^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v2.19.2",
    "php-parallel-lint/php-var-dump-check": "^0.5.0",
    "phpstan/phpstan": "^0.12",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon --no-progress -vvv"
  },
  "autoload": {
    "psr-4": {
      "Viterbit\\PdfImagesExtractor\\": "src"
    }
  }
}
